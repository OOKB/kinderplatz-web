        "houdini": "^1.1.3",
        "houdini-svelte": "^1.1.3",
        "postcss": "^8.4.21",
        "prettier": "^2.8.7",
        "prettier-plugin-svelte": "^2.10.0",
        "svelte": "^3.57.0",
        "svelte-check": "^3.1.4",
        "tailwindcss": "^3.2.7",
        "typescript": "^5.0.2",
